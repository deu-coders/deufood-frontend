{
	"env": {
		"browser": true,
		"es2021": true
	},
	"plugins": [
		"@typescript-eslint",
		"react"
	],
	"settings": {
		"pragma": "React",
		"version": "detect"
	},
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"plugin:react/recommended",
		"prettier"
	],
	"rules": {
		"indent": "off",
		"react/prop-types": "off"
	}
}